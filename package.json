{
  "name": "boilerplate",
  "version": "0.0.1",
  "main": "index.js",
  "author": "engineering@sixfold.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "copy-files": "cpy src/data/airports.dat build/data/ --flat && cpy src/data/routes.dat build/data/ --flat",
    "build": "tsc && yarn copy-files",
    "build:clean": "rm -rf ./build && yarn build",
    "dev": "TS_NODE_PROJECT='./tsconfig.json' ts-node-dev --respawn ./src src/index.ts",
    "test": "jest",
    "start": "yarn build && node build/index.js",
    "lint": "eslint 'src/**/*.{ts,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,json}'",
    "format": "prettier --write 'src/**/*.{ts,json}' --config ./.prettierrc"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "cpy-cli": "^4",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.6.2",
    "jest-extended": "^3.2.3",
    "prettier": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/express": "^4.17.15",
    "@types/kdbush": "^3.0.2",
    "csv-parse": "^4.14.1",
    "express": "^4.18.2",
    "heap-js": "^2.3.0",
    "kdbush": "^3.0.0",
    "morgan": "^1.10.0"
  },
  "resolutions": {
    "**/debug": "^4"
  }
}
